{
  "name": "cori-data-api",
  "version": "0.0.1",
  "description": "CORI Data API Project",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:schema": "npm run play -w schemas",
    "start:api": "npm run start -w infrastructure",
    "build": "npm run build --ws",
    "build:infra": "npm run build -w infrastructure",
    "build:schemas": "npm run build -w schemas",
    "deploy": "npm run build:all && npm run deploy -w infrastructure",
    "synth": "npm run build:all && npm run synth -w infrastructure",
    "publish:schemas": "npm run build:schemas && npm run hive-publish -w schemas",
    "diff": "npm run diff --w packages/infrastructure",
    "ts": "npm run node -- -r ts-node/register",
    "build:python-layer": "bash ./buildPythonDependencyLayer.sh",
    "lint": "eslint ./packages ./config",
    "lint:fix": "eslint --fix ."
  },
  "author": "Merging Futures LLC.",
  "license": "ISC",
  "workspaces": [
    "packages/schemas",
    "packages/infrastructure"
  ],
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "cdk": "2.20.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-import-resolver-typescript": "^3.1.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "prettier": "^2.7.1"
  }
}
